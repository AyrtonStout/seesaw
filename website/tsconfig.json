{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "strict": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"]
}